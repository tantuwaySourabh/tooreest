<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="edittextfont" format="string" />
        <attr name="edittextfontHint" format="string" />
        <attr name="edittextpwd" format="string" />
        <attr name="editcharpwd" format="string" />
    </declare-styleable>
    <declare-styleable name="ParallaxView">
        <attr name="speed" format="dimension" />
        <attr name="src" format="reference" />
        <attr name="sceneLength" format="integer" />
        <attr name="randomness" format="reference" />
        <attr name="initialState" format="enum">
            <enum name="started" value="0" />
            <enum name="stopped" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <attr name="shadow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomRatingBar">
        <attr name="indicator" format="boolean" />
        <attr name="rating" format="float" />
        <attr name="type" format="enum">
            <enum name="normal" value="0" />
            <enum name="small" value="1" />
        </attr>
        <attr name="coloredRatingBarStyle" format="reference" />
        <!-- Indicator RatingBar style. -->
        <attr name="coloredRatingBarStyleIndicator" format="reference" />
        <!-- Small RatingBar style. -->
        <attr name="coloredRatingBarStyleSmall" format="reference" />
        <!-- Small indicator RatingBar style. -->
        <attr name="coloredRatingBarStyleSmallIndicator" format="reference" />
    </declare-styleable>

    <declare-styleable name="GIFView"></declare-styleable>


</resources>
